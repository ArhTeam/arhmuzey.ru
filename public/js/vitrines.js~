$(document).ready(function() {
			/* Изменяет окно и скорость закрытия*/
			// Свой стиль, закрыть при нажатии, изменить окно и цвет фона 
			$(".fancybox-effects-c").fancybox({
				wrapCSS    : 'fancybox-custom',
				closeClick : true,
				openEffect : 'none',

				helpers : {
					overlay : {
						css : {
							'background' : 'rgba(238,238,238,0.85)'   /* фон при открытии, серый */
						}
					}
				}
			});
		});
